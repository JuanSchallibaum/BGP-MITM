!
!
service integrated-vtysh-config
!
interface erspan0
!
interface eth0
 ip address 30.0.0.1/30
!
interface eth1
 ip address 30.0.0.5/30
!
interface gre0
!
interface gretap0
!
interface lo
!
router bgp 30
 bgp router-id 66.66.66.18
 network 30.0.0.0/8
 neighbor 30.0.0.2 remote-as 20
 neighbor 30.0.0.6 remote-as 666
!
 address-family ipv6
 exit-address-family
 exit
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
